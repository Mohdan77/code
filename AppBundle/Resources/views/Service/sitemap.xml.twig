<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ absolute_url(path('home')) }}</loc>
        <lastmod>{{ home.updatedAt|date('c') }}</lastmod>
    </url>
    {% for item in locations %}
        <url>
            <loc>{{ link(item.url, true) }}</loc>
            <lastmod>{{ item.updatedAt|date('c') }}</lastmod>
        </url>
    {% endfor %}
</urlset>